<div class="wellclear">
<% if user_signed_in? && current_user.super_admin? %>
  <p id="notice"><%= notice %></p>

  <h1>Listing Feeds</h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Url</th>
        <th>Summary</th>
        <th>Image url</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @feeds.each do |feed| %>
        <tr>
          <td><%= feed.name %></td>
          <td><%= feed.url %></td>
          <td><%= feed.summary %></td>
          <td><%= feed.image_url %></td>
          <td><%= link_to 'Show', feed %></td>
          <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
          <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Feed', new_feed_path %>
<% else %>
  <h1>Nothing to see here</h1>
<% end %>
</div>